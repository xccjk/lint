    "src",
    "es",
    "lib",
    "plugins"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/generator": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/traverse": "^7.11.5",
    "hotkeys-js": "^3.8.1",
    "loader-utils": "^2.0.0",
