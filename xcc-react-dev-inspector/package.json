    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-reconciler": "^0.25.1",
