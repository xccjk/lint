  "description": "dev-tool for inspect react components and jump to local IDE for component code.",
  "main": "es/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf build dist es lib plugins",
    "dev": "nodemon -w src -e ts,tsx -x 'npm run build && (cd site; npm run dev)'",
